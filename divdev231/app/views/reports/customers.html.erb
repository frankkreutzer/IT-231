<h1>Our Customers</h1>
<table border="1">
	<tr>
		<th> Customer Name </th>
		<th> City </th>
		<th> State </th>
		<th> Email </th>
		<th> Phone </th>
	</tr>

<% @customerlist.each do |c| %>
	<tr>
		<td> <%= c.name %> </td>
		<td> <%= c.city %> </td>
		<td> <%= c.state %> </td>
		<td> <%= c.email %> </td>
		<td><%= number_to_phone(c.phone) %></td>

	</tr>
<% end %>
</table>
